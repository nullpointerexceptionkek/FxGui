            <RadioButton fx:id="none" layoutX="180.0" layoutY="346.0" mnemonicParsing="false" onAction="#getTimeStampMode" prefHeight="17.0" prefWidth="99.0" text="None" textFill="WHITE" toggleGroup="$timestampmode" />
