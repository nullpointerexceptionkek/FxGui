<StackPane fx:id="stackPane" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Gui.config.ready.ConfigController">
   <children>
      <AnchorPane fx:id="Anchorroot" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="540.0" prefWidth="334.0" style="-fx-background-color: #2C2F33;">
         <children>
            <Label fx:id="titleLabel" alignment="CENTER" layoutX="50.0" layoutY="14.0" prefHeight="32.0" prefWidth="234.0" text="Custom Discord RP" textFill="WHITE">
               <font>
                  <Font name="Lucida Sans Unicode" size="20.0" />
               </font>
            </Label>
            <Button fx:id="callbackButton" layoutX="116.0" layoutY="501.0" mnemonicParsing="false" onAction="#switchToCallBack" text="Launch Call Back" />
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="106.0" layoutY="46.0" prefHeight="18.0" prefWidth="123.0" text="Application ID" />
            <TextField fx:id="appID" layoutX="68.0" layoutY="64.0" prefHeight="26.0" prefWidth="202.0" promptText="Enter Discord Application ID" />
